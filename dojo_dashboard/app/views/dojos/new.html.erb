<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>New Dojo</title>
    </head>
    <body>
        <form action="/dojos/create" method='post'>
            <%= hidden_field_tag :authenticity_token, form_authenticity_token -%>
            <div class="field">
                <label for="dojo_branch">Branch</label>
                <input id="dojo_branch" name="dojo[branch]" type="text" />
            </div>
            <div class="field">
                <label for="dojo_street">Street</label>
                <input id="dojo_street" name="dojo[street]" type="text" />
            </div>
            <div class="field">
                <label for="dojo_city">City</label>
                <input id="dojo_city" name="dojo[city]" type="text" />
            </div>
            <div class="field">
                <label for="dojo_state">State</label>
                <input id="dojo_state" name="dojo[state]" type="text" />
            </div>
            <div class="actions">
                <input name="commit" type="submit" value="Create"/>
            </div>
        </form>
    </body>
</html>
